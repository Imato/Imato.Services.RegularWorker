{
    "ConnectionStrings": {
        "mssql": "Data Source=localhost;Initial Catalog=Test;Persist Security Info=True;User ID={USER_NAME};Password={USER_PASSWORD};"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "Workers": {
        "ConnectionString": "mssql",
        "StatusTimeout": 60000,
        "LogWorker": {
            "StartInterval": 15000,
            "RunOn": "EveryWhere", /* EveryWhere, PrimaryServer, SecondaryServer, SecondaryServerFirst, Single */
            "Enabled": true,
            "Server": "null or server name",
            "MaxExecutionTime": 300000,
            "RestartInterval": 3600000
        }
    }
}